<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="APIs">
      <section name="LanguageAPI" type="System.Configuration.NameValueSectionHandler" />
      <section name="LyricsAPI" type="System.Configuration.NameValueSectionHandler" />
      <section name="MusicAPI" type="System.Configuration.NameValueSectionHandler" />
      <section name="PhotographyAPI" type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
  </configSections>
  <APIs>
    <LanguageAPI>
      <add key="MSTextAnalyticsK1" value="YourKeyHere"/>
    </LanguageAPI>
    <LyricsAPI>
      <add key="MusicXMatchKey" value="YourKeyHere"/>
    </LyricsAPI>
    <MusicAPI>
      <add key="SpotifyKey" value="YourKeyHere"/>
      <add key="SpotifyRedirectUrl" value="http://localhost"/>
      <add key="SpotifyListeningPort" value="8000"/>
    </MusicAPI>
    <PhotographyAPI>
      <add key="FlickrAPI" value="YourKeyHere"/>
    </PhotographyAPI>
  </APIs>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>